{
  "name": "astrobin-ng",
  "version": "0.1.0",
  "description": "AstroBin NG",
  "license": "AGPL-3.0",
  "repository": "https://github.com/astrobin/astrobin-ng",
  "scripts": {
    "postinstall": "node patch-iconv.js && node patch-ng-devkit.js",
    "ng": ".node_modules/.bin/ng",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0 --port 4400 --extra-webpack-config webpack.partial.js",
    "start:prod": "ng serve --host 0.0.0.0 --port 4400 --configuration=local-production  --extra-webpack-config webpack.partial.js",
    "build": "ng build  --extra-webpack-config webpack.partial.js",
    "build:stats": "ng build  --extra-webpack-config webpack.partial.js --stats-json",
    "build:prod": "ng build --prod --aot  --extra-webpack-config webpack.partial.js",
    "test": "jest --config jest.config.js",
    "test:cov": "jest --config jest.config.js --coverage",
    "cy:wait-run": "./node_modules/.bin/wait-on http-get://localhost:4400/ && cypress run",
    "cy:wait-run-record": "./node_modules/.bin/wait-on http-get://localhost:4400/ && cross-var cypress run --record --key $CYPRESS_KEY",
    "cy:serve-and-run": "node_modules/.bin/run-p -r \"start\" \"cy:wait-run\"",
    "cy:serve-and-run-record": "node_modules/.bin/run-p -r \"start\" \"cy:wait-run-record\"",
    "analyze": "webpack-bundle-analyzer dist/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "10.0.0",
    "@angular/animations": "9.1.12",
    "@angular/common": "9.1.12",
    "@angular/compiler": "9.1.12",
    "@angular/core": "9.1.12",
    "@angular/forms": "9.1.12",
    "@angular/localize": "9.1.12",
    "@angular/platform-browser": "9.1.12",
    "@angular/platform-browser-dynamic": "9.1.12",
    "@angular/router": "9.1.12",
    "@fjnr/ngx-translate-po-http-loader": "3.1.5",
    "@fortawesome/angular-fontawesome": "0.7.0",
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@ng-bootstrap/ng-bootstrap": "6.1.0",
    "@ngx-formly/bootstrap": "5.8.0",
    "@ngx-formly/core": "5.8.0",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "bootstrap": "4.5.0",
    "bootswatch": "4.5.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "core-js": "3.6.5",
    "dotenv": "8.1.0",
    "intl": "1.2.5",
    "ngx-cookie-service": "3.0.4",
    "ngx-timeago": "1.0.4",
    "ngx-toastr": "12.1.0",
    "ngx-uploadx": "3.4.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.6.2",
    "tslib": "2.0.1",
    "webpack-bundle-tracker": "0.4.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.9",
    "@angular/cli": "9.1.9",
    "@angular/compiler-cli": "9.1.12",
    "@angular/language-service": "9.1.12",
    "@types/dotenv": "6.1.1",
    "@types/jest": "26.0.13",
    "@types/node": "12.12.53",
    "@types/supertest": "2.0.10",
    "codelyzer": "6.0.0",
    "concurrently": "5.3.0",
    "cross-var": "1.1.0",
    "cypress": "4.12.1",
    "faker": "5.1.0",
    "jest": "26.4.2",
    "jest-preset-angular": "7.1.1",
    "js-beautify": "1.13.0",
    "ng-mocks": "10.1.2",
    "ngx-build-plus": "9.0.6",
    "nodemon": "1.19.3",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "protractor": "7.0.0",
    "stylelint": "13.6.1",
    "supertest": "4.0.2",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "tslint-angular": "3.0.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3",
    "wait-on": "5.2.0",
    "webpack-bundle-analyzer": "3.8.0"
  },
  "jest": {
    "projects": [
      "jest.config.js"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
