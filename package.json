{
  "name": "astrobin-ng",
  "version": "0.1.0",
  "description": "AstroBin NG",
  "license": "AGPL-3.0",
  "repository": "https://github.com/astrobin/astrobin-ng",
  "scripts": {
    "postinstall": "node patch-iconv.js && node patch-ng-devkit.js",
    "ng": ".node_modules/.bin/ng",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0 --port 4400 --extra-webpack-config webpack.partial.js",
    "start:prod": "ng serve --host 0.0.0.0 --port 4400 --configuration=local-production  --extra-webpack-config webpack.partial.js",
    "build": "ng build  --extra-webpack-config webpack.partial.js",
    "build:prod": "ng build --prod --aot  --extra-webpack-config webpack.partial.js",
    "test": "jest --config jest.config.js",
    "test:cov": "jest --config jest.config.js --coverage",
    "cy:wait-run": "./node_modules/.bin/wait-on http-get://localhost:4400/ && cypress run",
    "cy:wait-run-record": "./node_modules/.bin/wait-on http-get://localhost:4400/ && cross-var cypress run --record --key $CYPRESS_KEY",
    "cy:serve-and-run": "node_modules/.bin/run-p -r \"start\" \"cy:wait-run\"",
    "cy:serve-and-run-record": "node_modules/.bin/run-p -r \"start\" \"cy:wait-run-record\""
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "9.1.0",
    "@angular/animations": "9.1.3",
    "@angular/common": "9.1.3",
    "@angular/compiler": "9.1.3",
    "@angular/core": "9.1.3",
    "@angular/forms": "9.1.3",
    "@angular/localize": "9.1.3",
    "@angular/platform-browser": "9.1.3",
    "@angular/platform-browser-dynamic": "9.1.3",
    "@angular/router": "9.1.3",
    "@fjnr/ngx-translate-po-http-loader": "3.1.2",
    "@fortawesome/angular-fontawesome": "0.6.1",
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@ng-bootstrap/ng-bootstrap": "6.1.0",
    "@ngx-formly/bootstrap": "5.6.0",
    "@ngx-formly/core": "5.6.0",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "bootstrap": "4.3.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "core-js": "3.6.5",
    "dotenv": "8.1.0",
    "intl": "1.2.5",
    "ngx-cookie-service": "3.0.4",
    "ngx-timeago": "1.0.4",
    "ngx-toastr": "12.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "webpack-bundle-tracker": "0.4.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.3",
    "@angular/cli": "9.1.3",
    "@angular/compiler-cli": "9.1.3",
    "@angular/language-service": "9.1.3",
    "@types/dotenv": "6.1.1",
    "@types/jest": "25.2.1",
    "@types/node": "12.12.37",
    "@types/supertest": "2.0.7",
    "codelyzer": "5.2.2",
    "concurrently": "5.2.0",
    "cross-var": "1.1.0",
    "cypress": "4.5.0",
    "faker": "4.1.0",
    "jest": "25.5.1",
    "jest-preset-angular": "7.1.1",
    "js-beautify": "1.11.0",
    "ng-mocks": "9.2.0",
    "ngx-build-plus": "9.0.6",
    "nodemon": "1.19.3",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "protractor": "5.4.4",
    "sqlite3": "4.2.0",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "ts-node": "8.9.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "tslint-angular": "3.0.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3",
    "wait-on": "3.3.0"
  },
  "jest": {
    "projects": [
      "jest.config.js"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
