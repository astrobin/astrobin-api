{
  "name": "astrobin-ng",
  "version": "0.1.0",
  "description": "AstroBin NG",
  "license": "AGPL-3.0",
  "repository": "https://github.com/astrobin/astrobin-ng",
  "scripts": {
    "postinstall": "node patch-iconv.js && node patch-ng-devkit.js & ngcc",
    "ng": ".node_modules/.bin/ng",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0 --port 4400 --extra-webpack-config webpack.partial.js",
    "start:prod": "ng serve --host 0.0.0.0 --port 4400 --configuration=local-production  --extra-webpack-config webpack.partial.js",
    "build": "ng build  --extra-webpack-config webpack.partial.js",
    "build:stats": "ng build  --extra-webpack-config webpack.partial.js --stats-json",
    "build:prod": "ng build --prod --aot  --extra-webpack-config webpack.partial.js",
    "test": "jest --config jest.config.js",
    "test:cov": "jest --config jest.config.js --coverage",
    "cy:wait-run": "./node_modules/.bin/wait-on http-get://localhost:4400/ && cypress run",
    "cy:wait-run-record": "./node_modules/.bin/wait-on http-get://localhost:4400/ && cross-var cypress run --record --key $CYPRESS_KEY",
    "cy:serve-and-run": "node_modules/.bin/run-p -r \"start\" \"cy:wait-run\"",
    "cy:serve-and-run-record": "node_modules/.bin/run-p -r \"start\" \"cy:wait-run-record\"",
    "analyze": "webpack-bundle-analyzer dist/stats-es2015.json",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.po --replace --format pot --sort",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{de,fr,es,it,pt}.po --clean --format pot --sort"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular/animations": "11.0.3",
    "@angular/common": "11.0.3",
    "@angular/compiler": "11.0.3",
    "@angular/core": "11.0.3",
    "@angular/forms": "11.0.3",
    "@angular/localize": "11.0.3",
    "@angular/platform-browser": "11.0.3",
    "@angular/platform-browser-dynamic": "11.0.3",
    "@angular/router": "11.0.3",
    "@fjnr/ngx-translate-po-http-loader": "3.1.5",
    "@fortawesome/angular-fontawesome": "0.8.0",
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@ng-bootstrap/ng-bootstrap": "8.0.0",
    "@ngx-formly/bootstrap": "5.10.6",
    "@ngx-formly/core": "5.10.6",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "bootstrap": "4.5.3",
    "bootswatch": "4.5.3",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "core-js": "3.8.1",
    "dotenv": "8.1.0",
    "intl": "1.2.5",
    "ngx-cookie-service": "11.0.2",
    "ngx-filesize": "2.0.16",
    "ngx-timeago": "2.0.0",
    "ngx-toastr": "13.2.0",
    "ngx-uploadx": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "webpack-bundle-tracker": "0.4.3",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.3",
    "@angular/cli": "11.0.3",
    "@angular/compiler-cli": "11.0.3",
    "@angular/language-service": "11.0.3",
    "@types/dotenv": "6.1.1",
    "@types/jest": "26.0.16",
    "@types/node": "12.19.8",
    "@types/supertest": "2.0.10",
    "codelyzer": "6.0.1",
    "concurrently": "5.3.0",
    "cross-var": "1.1.0",
    "cypress": "5.6.0",
    "faker": "5.1.0",
    "jest": "26.6.3",
    "jest-preset-angular": "8.3.2",
    "js-beautify": "1.13.0",
    "ng-mocks": "11.2.2",
    "ngx-build-plus": "10.1.1",
    "nodemon": "1.19.3",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "protractor": "7.0.0",
    "stylelint": "13.8.0",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "tslint-angular": "3.0.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.0.5",
    "wait-on": "5.2.0",
    "webpack-bundle-analyzer": "4.2.0"
  },
  "jest": {
    "projects": [
      "jest.config.js"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
